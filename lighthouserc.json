{
	"ci": {
		"collect": {
			"staticDistDir": "build",
			"settings": {
				"preset": "desktop",
				"output": ["html", "json"]
			}
		},
		"assert": {
			"assertions": {
				"categories:accessibility": ["error", { "minScore": 0.98 }],
				"categories:seo": ["error", { "minScore": 0.95 }]
			}
		},
		"upload": {
			"target": "filesystem",
			"outputDir": ".lighthouseci"
		}
	}
}
